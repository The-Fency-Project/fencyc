func factorial(n: int) -> int {
    if n <= 1 {
        return 1;
    }
    return n * factorial(n - 1);
}

func add(a: int, b: int) -> int {
    return a + b;
}

override func add(a: float, b: float) -> float {
    return a + b;
}
